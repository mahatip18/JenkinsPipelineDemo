pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
            }
            stage('Test') {
            steps {
                echo 'Testing'
            }
            stage('Develop') {
            steps {
                echo 'Developing'
            }
            stage('Release') {
            steps {
                echo 'Releasing'
            }
    }
    
}
       
